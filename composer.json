{
    "name": "weboptic/laravel-flash-notifications",
    "version": "1.0",
    "license": "MIT",
    "keywords": ["laravel", "flash", "notifications", "alerts", "bootstrap", "messages"],
    "description": "Easy way to display Flash Notifications in Laravel 5.",
    "authors": [
        {
            "name": "Szymon Krajewski",
            "email": "szykra@gmail.com"
        },
        {
            "name": "Shane Deakin",
            "email": "shane@weboptic.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0",
        "illuminate/session": "^10.0",
        "illuminate/contracts": "^10.0"
    },
    "autoload": {
        "psr-0": {
            "Weboptic\\Notifications": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Weboptic\\Notifications\\NotificationServiceProvider"
            ],
            "aliases": {
                "Flash": "Weboptic\\Notifications\\Flash"
            }
        }
    },
    "minimum-stability": "stable"
}
